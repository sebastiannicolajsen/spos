{
   "name": "spos",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "clean": "rimraf dist",
      "build": "tsc",
      "watch:build": "tsc --watch",
      "watch:server": "nodemon './build/index.js' --watch './build'",
      "start": "ts-node src/index.ts",
      "test": "NODE_ENV=test JWT_SECRET=testsecret LOG_TYPE=console jest --runInBand",
      "seeding": "ts-node src/utils/seeding/index.ts",
      "typeorm": "source .env && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d src/repositories/data-source.ts",
      "migration:create": "cross-var ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/migrations/$npm_config_name",
      "migration:generate": "cross-var npm run typeorm -- migration:generate ./src/migrations/$npm_config_name",
      "migration:run": "npm run build && npm run typeorm -- migration:run",
      "migration:revert": "npm run typeorm -- migration:revert"
   },
   "author": "",
   "license": "ISC",
   "devDependencies": {
      "@types/express": "^4.17.14",
      "@types/node": "^16.11.63",
      "@types/sqlite3": "^3.1.8",
      "@typescript-eslint/eslint-plugin": "^5.38.0",
      "eslint": "^8.24.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-config-standard-with-typescript": "^23.0.0",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-n": "^15.3.0",
      "eslint-plugin-prettier": "^4.2.1",
      "eslint-plugin-promise": "^6.0.1",
      "jest": "^29.1.2",
      "prettier": "^2.7.1",
      "sqlite": "^4.1.2",
      "sqlite3": "^5.1.2",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@types/axios": "^0.14.0",
      "@types/body-parser": "^1.19.2",
      "@types/cors": "^2.8.12",
      "@types/cron": "^2.0.0",
      "@types/express-validator": "^3.0.0",
      "@types/ioredis": "^4.28.10",
      "@types/jest": "^29.1.2",
      "@types/lodash": "^4.14.186",
      "@types/passport": "^1.0.11",
      "@types/passport-jwt": "^3.0.7",
      "@types/passport-local": "^1.0.34",
      "@types/winston": "^2.4.4",
