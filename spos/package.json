{
   "name": "spos",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "clean": "rimraf dist",
      "build": "tsc",
      "watch:build": "tsc --watch",
      "watch:server": "nodemon './dist/index.js' --watch './dist'",
      "start": "ts-node src/index.ts",
      "seeding": "ts-node src/seeding/index.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d src/repositories/data-source.ts",
      "migration:create": "cross-var ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/migrations/$npm_config_name",
      "migration:generate": "cross-var npm run typeorm -- migration:generate ./src/migrations/$npm_config_name",
      "migration:run": "npm run build && npm run typeorm -- migration:run",
      "migration:revert": "npm run typeorm -- migration:revert"
   },
   "author": "",
   "license": "ISC",
   "devDependencies": {
      "@types/express": "^4.17.1",
      "@types/node": "^16.11.10",
      "@typescript-eslint/eslint-plugin": "^5.38.0",
      "eslint": "^8.24.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-config-standard-with-typescript": "^23.0.0",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-n": "^15.3.0",
      "eslint-plugin-prettier": "^4.2.1",
      "eslint-plugin-promise": "^6.0.1",
      "prettier": "^2.7.1",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@types/bcrypt": "^5.0.0",
      "bcrypt": "^5.0.1",
      "cross-var": "^1.1.0",
      "express": "^4.18.1",
      "nodemon": "^2.0.20",
      "npm-run-all": "^4.1.5",
      "passport": "^0.6.0",
      "pg": "^8.8.0",
      "reflect-metadata": "^0.1.13",
      "tsconfig-paths": "^4.1.0",
      "typeorm": "0.3.10"
   }
}
